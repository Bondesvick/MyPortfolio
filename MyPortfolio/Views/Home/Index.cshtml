@model ViewModel

@{
    ViewData["Title"] = "Home Page";
}

<section class="banner-section s4 two-home" id="home">
    <div class="container">
        <div class="content-text position-relative">
            <div class="animate-element wow delay5 fadeInDown" data-wow-delay="0.3s">
                <div class="tag color-d1 lt-sp53 mg-b34">Full-Stack Engineer</div>

                <h1 class="cd-headline clip is-full-width title color-d11 mg-b41">
                    <span>Hi, </span>
                    <span class="cd-words-wrapper color-d6">
                        <b class="is-visible">I’m @Model.MyDetails.FirstName!</b>
                        <b class="engnr-txt" style="font-size: 6vw;">I’m a @Model.MyDetails.Occupation!</b>
                    </span>
                </h1>

                <p class="color-d13 lt-sp02 mg-b51">
                    Hi, i am @Model.MyDetails.FirstName, a professional @Model.MyDetails.Occupation with over @Model.MyDetails.YearsOfExperience years <br>experience on: <br />@Model.MyDetails.Skills.<br /> Need any help?
                </p>
            </div>
            <div class="animate-element wow delay5 fadeInUp" data-wow-delay="0.3s">
                <div class="fl-btn btn-general color-s2 btn-hv-border">
                    <a href="@Model.MyDetails.ResumeLink" target="_blank" class="f-w500 color-d6 border-corner5 lt-sp01 text-two">Download Resume</a>
                </div>
            </div>
        </div>
        <div class="featured-post border-corner-bl">
            <img src="@Model.MyDetails.ProfileImage1" alt="images">
            <div class="site-list has-border text-center animate-element wow delay5 fadeInUp" data-wow-delay="0.3s">
                <a href="@Model.MyDetails.Twitter" class="bg-s1"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                <a href="@Model.MyDetails.Facebook" class="bg-s2"><i class="fa  fa-facebook-square" aria-hidden="true"></i></a>
                <a href="@Model.MyDetails.GitHub" class="bg-s3"><i class="fa fa-github-square" aria-hidden="true"></i></a>
                <a href="@Model.MyDetails.Instagram" class="bg-s3"><span class="fa fa-instagram"></span></a>
                <a href="@Model.MyDetails.LinkedIn" class="bg-s1"><span class="fa fa-linkedin-square"></span></a>
            </div>
        </div>
    </div>
    <div class="iconbox-s4 t2 color-s2 border-corner5 animate-element wow delay5 fadeZooming" data-wow-delay="0.3s">
        <div class="avatar position-relative">
            <img src="~/ThemeDoc/images/footer/01.jpg" alt="images">
        </div>

        <!---MAil to-->
        <div class="content-inner">
            <h4 class="name lt-sp04 mg-b16 text-white position-relative">Available for Project</h4>
            <div class="fl-btn btn-general">
                <a href="mailto: bondesvick@gmail.com" class="text-white border-corner2">Hire Me</a>
            </div>
        </div>
    </div>
</section><!-- banner-section -->

<section class="about-me t2 s3 shape-am position-relative" id="about">
    <div class="section-icons">
        <div class="shape-one"><img src="~/ThemeDoc/images/testimonial/01.png" alt="images"></div>
        <div class="shape-two"><img src="~/ThemeDoc/images/testimonial/02.png" alt="images"></div>
        <div class="shape-three"><img src="~/ThemeDoc/images/testimonial/03.png" alt="images"></div>
        <div class="shape-four"><img src="~/ThemeDoc/images/testimonial/04.png" alt="images"></div>
        <div class="shape-five"><img src="~/ThemeDoc/images/testimonial/05.png" alt="images"></div>
        <div class="shape-six"><img src="~/ThemeDoc/images/testimonial/06.png" alt="images"></div>
    </div>
    <div class="container">
        <div class="row position-relative">
            <div class="col-lg-7 animate-element wow delay5 fadeInDown">
                <div class="flat-title t2">
                    <h4 class="sub-title mg-b22">About Me</h4>
                    <h2 class="title-section color-d11 mg-b41">Hi, I am here to help with your @Model.MyDetails.Skills projects!</h2>
                    <div class="description">
                        <p class="color-d13 lt-sp01">
                            @Model.MyDetails.Introduction1
                        </p>
                        <p class="color-d13 lt-sp01">
                            @Model.MyDetails.Introduction2
                        </p>
                    </div>
                </div>
            </div>
            <div class="fact">
                <div class="d-md-flex mg-b40">
                    <div class="col-left">
                        <div class="f-item color-s2 counter mg-f-item1">
                            <div class="counter-box bg-s2 d-flex justify-content-center align-items-center">
                                <div class="inner">
                                    <span class="numb-count" data-from="0" data-to="284" data-speed="2000" data-inviewport="yes">284</span>
                                    <p class="color-d13">Happy Client</p>
                                </div>
                            </div>
                        </div>
                        <div class="f-item color-s3 counter mg-f-item2">
                            <div class="counter-box bg-s2 d-flex justify-content-center align-items-center">
                                <div class="inner">
                                    <span class="numb-count" data-from="0" data-to="165" data-speed="2000" data-inviewport="yes">165</span>
                                    <p class="color-d13">Review</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-right">
                        <div class="f-item color-s4 counter mg-f-item3">
                            <div class="counter-box bg-s2 d-flex justify-content-center align-items-center">
                                <div class="inner">
                                    <span class="numb-count" data-from="0" data-to="379" data-speed="2000" data-inviewport="yes">379</span>
                                    <p class="color-d13">Project Complete</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="btn-contact bg-s2 text-center">
                    <h4 class="title color-d11">Got a project? Let’s talk.</h4>
                    <a href="mailto: @Model.MyDetails.Email" class="email color-s2 color-d15">@Model.MyDetails.Email</a>
                </div>
            </div>
        </div>
    </div>
    <div class="featured-post animate-element wow delay5 fadeInUp" data-wow-delay="0.5s">
        <img src="~/ThemeDoc/images/section/51.png" alt="images">
    </div>
</section><!-- about-me -->

