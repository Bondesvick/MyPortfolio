@model ViewModel
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    @*<meta name="viewport" content="width=device-width, initial-scale=1.0" />*@

    <title>@ViewData["Title"] - MyPortfolio</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/bootstrap.css">
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    @*<link rel="stylesheet" href="~/css/site.css" />*@
    <!-- Template Style-->
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/font-awesome.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/owl.carousel.min.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/animate.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/icomoon.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/jquery.pagepiling.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/animation-text.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/style.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/shortcodes.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/responsive.css">
    <link rel="stylesheet" href="~/ThemeDoc/stylesheet/mobile-menu.css">
</head>
<body class="counter-scroll background-wrap-home4 fixed counter-scroll home4 has-one-page">
    <div id="loading-overlay">
        <div class="loader"></div>
    </div>
    <div id="mobile-menu-overlay"></div>
    <header id="header" class="header header-style4 has-menu-model">
        <div class="container">
            <div class="flex-header d-flex justify-content-between align-items-center">
                <div id="logo" class="logo">
                    <a asp-area="" asp-controller="Home" asp-action="Index" title="Logo">
                        <h3 class="widget-title larger lt-sp06 text-white" style="font-size: 40px; font-weight: bold !important">
                            <span style="color: #627FFF !important;">@Model.MyDetails.FirstName</span> @Model.MyDetails.SecondName
                        </h3>
                    </a>
                </div>
                <div dir="rtl" class="btn-menu mobile-header__menu-button">
                    <div class="line line-1"></div>
                    <div class="line line-2"></div>
                    <div class="line line-3"></div>
                    <div class="line line-4"></div>
                </div>
                <div class="content-menu d-lg-flex">
                    <div class="nav-wrap">
                        <nav id="mainnav" class="mainnav">
                            <ul class="menu ace-responsive-menu" data-menu-style="horizontal">

                                <li><a href="#about" class="click-model">About</a></li>
                                <li><a href="#services" class="click-model">Services</a></li>
                                <li><a href="#portfolio" class="click-model">Portfolio</a></li>
                                <li><a href="#testimonial" class="click-model">Testimonial</a></li>
                                <li><a href="#resume" class="click-model">Resume</a></li>
                            </ul>
                        </nav>
                    </div>
                </div>
                <div class="socials-list-hd s3 hv2">
                    <a href="@Model.MyDetails.Facebook"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                    <a href="@Model.MyDetails.Instagram"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                    <a href="@Model.MyDetails.Twitter"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                </div>
            </div>
        </div>
    </header><!-- header -->

    <div id="content" class="content">
        <div class="homepage-personal">
            @RenderBody()

            <div class="tf-modal-content justify-content-end">
                <footer id="footer" class="footer footer-s4">
                    <div class="container">
                        <div id="footer-widget" class="footer-widget-s3 border-bottom-s2 position-relative">
                            <h3 class="widget-title larger lt-sp06 text-white text-center">Let’s try me!</h3>
                            <div class="site-list site-list-center text-center">

                                <a href="@Model.MyDetails.Twitter" class="bg-s1"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                                <a href="@Model.MyDetails.Facebook" class="bg-s2"><i class="fa  fa-facebook-square" aria-hidden="true"></i></a>
                                <a href="@Model.MyDetails.GitHub" class="bg-s3"><i class="fa fa-github-square" aria-hidden="true"></i></a>
                                <a href="@Model.MyDetails.Instagram" class="bg-s3"><span class="fa fa-instagram"></span></a>
                                <a href="@Model.MyDetails.LinkedIn" class="bg-s1"><span class="fa fa-linkedin-square"></span></a>
                            </div>
                        </div>
                    </div>
                    <div id="bottom" class="bottom-s3">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="copyright lt-sp02">
                                        © Roy Design, 2020 All rights reserved.
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <ul class="widget-nav-menu color-s2">
                                        <li><a asp-area="" asp-controller="Home" asp-action="Index" title="Logo">Home</a></li>
                                        <li><a href="#about">About</a></li>
                                        <li><a href="#services">Services</a></li>
                                        <li><a href="#portfolio">Portfolio</a></li>
                                        <li><a href="#testimonial">Testimonial</a></li>
                                        <li><a href="#resume">Resume</a></li>
                                    </ul>
                                </div>
                                <div class="col-lg-2">
                                    <div class="socials-list color-s2">
                                        <a href="@Model.MyDetails.Facebook"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                                        <a href="@Model.MyDetails.Twitter"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                                        <a href="@Model.MyDetails.Instagram"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </footer><!-- footer -->
            </div>
        </div>
    </div>
    <a id="scroll-top"></a>

    @*<div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>*@

    @*<footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2020 - MyPortfolio - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>*@
    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

        <script src="~/js/site.js" asp-append-version="true"></script>*@

    <script src="~/ThemeDoc/javascript/jquery.min.js"></script>
    <script src="~/ThemeDoc/javascript/plugins.js"></script>
    <script src="~/ThemeDoc/javascript/jquery-ui.js"></script>
    <script src="~/ThemeDoc/javascript/owl.carousel.min.js"></script>
    <script src="~/ThemeDoc/javascript/jquery.pagepiling.min.js"></script>
    <script src="~/ThemeDoc/javascript/jquery.easy-pie-chart.js"></script>
    <script src="~/ThemeDoc/javascript/wow.min.js"></script>
    <script src="~/ThemeDoc/javascript/jquery-countTo.js"></script>
    <script src="~/ThemeDoc/javascript/animatedModal.min.js"></script>
    <script src="~/ThemeDoc/javascript/animation-text.js"></script>
    <script src="~/ThemeDoc/javascript/main.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>